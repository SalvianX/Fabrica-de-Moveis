package fabrica;

public class Sofa extends Movel {
	private int numLugares;
	public Sofa(String nome,int numLugares) {
		super(nome);
		this.numLugares = numLugares;
		
		
	}
	public String toString() {
		return "Sofa [numLugares=" + numLugares + "]";
	}
	public int getNumLugares() {
		return numLugares;
	}

}
